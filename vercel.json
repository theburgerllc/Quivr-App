{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "env": {
    "NODE_VERSION": "20"
  },
  "regions": ["iad1"],
  "functions": {
    "app/api/stripe/webhook/route.ts": {
      "maxDuration": 10
    },
    "app/api/billing/webhook/route.ts": {
      "maxDuration": 10
    },
    "app/api/media/upload/route.ts": {
      "maxDuration": 30
    }
  }
}